meta {
  name: user
  type: http
  seq: 1
}

get {
  url: {{protocol}}://{{host}}:{{port}}/v1/users/user/{{currentUserId}}
  body: json
  auth: none
}

headers {
  Origin: http://postman.local
}

script:pre-request {
  const cookie = bru.getVar("cookie");
  
  if(cookie) {
    req.setHeader("Cookie", cookie)
  }
}
